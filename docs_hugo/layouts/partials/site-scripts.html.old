{{- if eq hugo.Environment "production" -}}
	{{- with .Site.Params.cdn.bootstrap -}}
<script type="text/javascript" src="{{ .js | safeURL }}" {{ with .js_hash }}{{ printf " integrity=%q" . | safeHTMLAttr }} crossorigin="anonymous"{{ end }}></script>
	{{- end -}}
{{ else -}}
<script type="text/javascript" src="{{ `/assets/lib/bootstrap/dist/js/bootstrap.bundle.js` | absURL }}"></script>
{{- end -}}
<script type="text/javascript" src="{{ `/assets/js/studiokonkon.js` | absURL }}"></script>
<script type="text/javascript">
<!--
(function() {
   var sidebar = new konkon.Sidebar("#left-sidebar");

   function sidebarClose(event) {
      if (typeof sidebar.triggerAutoClose !== "undefined") {
         sidebar.triggerAutoClose(event)
      }
   }

   document.addEventListener("mousedown", sidebarClose, false)
   document.addEventListener("touchstart", sidebarClose, false)
})();
//-->
</script>
<!-- Asset Scripts //-->
{{- $main_opts := ( dict
   "targetPath" "assets/js/app.min.js"
   "bundle" true
   "platform" "browser"
   "globalName" "app"
   "format" "iife"
   "target" "es2015"
   "minify" true
) -}}
{{- $main_built := resources.Get "js/main-app.js" | js.Build $main_opts -}}
<script type="text/javascript" src="{{ $main_built.Permalink }}"></script>
